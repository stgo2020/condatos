<p id="notice"><%= notice %></p>

<h1>Listing Rubidevices </h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Identifier</th>
      <th>Creation</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @rubidevices.each do |rubidevice| %>

      <tr>
        <td><%= rubidevice.user %></td>
        <td><%= rubidevice.identifier %></td>
        <td><%= rubidevice.creation %></td>
        <td><%= rubidevice.owner %></td>
        <td><%= link_to 'Show', rubidevice %></td>
        <td><%= link_to 'Edit', edit_rubidevice_path(rubidevice) %></td>
        <td><%= link_to 'Destroy', rubidevice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rubidevice', new_rubidevice_path %>
